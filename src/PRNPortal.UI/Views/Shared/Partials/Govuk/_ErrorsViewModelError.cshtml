@model List<ErrorViewModel>

@if (Model.Count > 0)
{
    @for (int error=0; error < Model.Count; error++)
    {
        <p id="@Model[error].Key@error" class="govuk-error-message">
            <span class="govuk-visually-hidden">@SharedLocalizer["hidden_error"]</span> @Model[error].Message
        </p>
        
    }

}